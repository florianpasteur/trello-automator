{
  "manifest_version": 3,
  "name": "Trello Automator Extension",
  "version": "1.0.0",
  "description": "Add functionalities to trello",
  "short_name": "Trello Automator",
  "permissions": [
    "storage",
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "css": [
      ]
    }
  ],
  "action": {
    "default_icon": "icons/icon512.png",
    "default_title": "Open actions",
    "default_popup": "popup.html"
  },
  "icons": {
    "512": "icons/icon512.png",
    "128": "icons/icon128.png"
  }
}
